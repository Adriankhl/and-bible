{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8085",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build-debug": "cross-env NODE_ENV=debug vue-cli-service build --target app --inline-vue",
    "build-development": "cross-env NODE_ENV=development vue-cli-service build --target app --inline-vue",
    "build-old": "vue-cli-service build",
    "build-production": "cross-env NODE_ENV=production vue-cli-service build --target app --inline-vue",
    "lint:ci": "vue-cli-service lint --no-fix --no-color > lint-results.txt",
    "lint:silent": "vue-cli-service lint --no-fix",
    "test:ci": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~6.2.1",
    "@fortawesome/free-solid-svg-icons": "~6.2.1",
    "@fortawesome/vue-fontawesome": "~3.0.0-5",
    "@vue/runtime-core": "~3.2.45",
    "bible-passage-reference-parser": "~2.0.1",
    "color": "~4.2.3",
    "core-js": "~3.26.1",
    "lodash": "~4.17.21",
    "mitt": "~3.0.0",
    "sprintf-js": "~1.1.2",
    "vue": "~3.2.45",
    "vuedraggable": "~4.1.0"
  },
  "devDependencies": {
    "@types/lodash": "~4.14.190",
    "@babel/eslint-parser": "~7.19.1",
    "@types/color": "~3.0.3",
    "@types/jest": "~27.0.1",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli": "~5.0.8",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.0",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "~3.2.45",
    "@vue/eslint-config-typescript": "~11.0.2",
    "@vue/reactivity": "~3.2.45",
    "@vue/test-utils": "~2.2.4",
    "@vue/vue3-jest": "~27.0.0",
    "babel-jest": "~27.5.1",
    "cross-env": "~7.0.3",
    "eslint": "~8.28.0",
    "jest-raw-loader": "~1.0.1",
    "jsdom": "~16.6.0",
    "json-loader": "~0.5.7",
    "npm": "~9.1.2",
    "raw-loader": "~4.0.2",
    "sass": "~1.56.1",
    "sass-loader": "~13.2.0",
    "ts-jest": "~27.0.4",
    "typescript": "~4.9.3",
    "vue-cli-plugin-webpack-bundle-analyzer": "~4.0.0",
    "vue-cli-plugin-yaml": "~1.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "strict": 1,
      "camelcase": [
        "error"
      ],
      "no-var": [
        "error"
      ],
      "require-atomic-updates": "off",
      "vue/multi-word-component-names": "off",
      "vue/no-reserved-component-names": "off",
      "vue/script-indent": [
        "error",
        2,
        {
          "baseIndent": 0,
          "switchCase": 1
        }
      ],
      "vue/html-indent": [
        "error",
        2,
        {
          "baseIndent": 1
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  }
}
