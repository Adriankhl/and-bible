{
  "name": "bibleview-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "andbible-build": "vue-cli-service build --target app --formats umd-min --inline-vue",
    "andbible-build-old": "vue-cli-service build --target app --formats umd-min --inline-vue --name BibleView src/main.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "dom-highlight-range": "~3.0.1",
    "sprintf-js": "~1.1.2",
    "jquery": "~3.5.0",
