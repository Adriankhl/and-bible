    "vue": "~3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "npm": "^6.14.8",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.0"
  },
  "eslintConfig": {
